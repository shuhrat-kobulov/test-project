        "@types/react": "^19.2.5",
        "@types/react-dom": "^19.2.3",
        "@typescript-eslint/eslint-plugin": "^8.52.0",
        "@typescript-eslint/parser": "^8.52.0",
        "@vitejs/plugin-react": "^5.1.1",
        "babel-plugin-react-compiler": "^1.0.0",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
